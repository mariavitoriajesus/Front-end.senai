Algoritmo "repeticoes"

Var
cont, cont2, cont3 : inteiro

Inicio
//while
enquanto (cont <=10) faca
   escreval(cont)
   cont <- cont + 1
fimEnquanto

//for
escreval("PARA FOR")
   para cont2 <- 1 ate 10 passo 1 faca
   escreval(cont2)
fimPara

//until
escreval("REPETIR UNTIL")
repita
   escreval(cont3)
   cont3 <- cont3 +1
ate(cont3 = 11)


Fimalgoritmo