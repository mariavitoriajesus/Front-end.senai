Algoritmo "procedimento"

Var
procedimento teste()
   VAR
nome : caractere
   INICIO
   leia(nome)
   escreval(nome)
fimProcedimento

funcao nome() : caractere
      VAR
   primeiroN, segundoN : caractere
      INICIO
   leia(primeiroN)
   leia(segundoN)
   retorne primeiroN
fimFuncao

Inicio
escreval(nome())


Fimalgoritmo